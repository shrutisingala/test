
public class DemoJagged {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [][] values = new int[3][];
		
		values[0] = new int[4];
		values[0][0] = 1;
		values[0][0] = 1;
		values[0][0] = 1;
		values[0][0] = 1;
		
		values[0] = new int[4];
		values[0][0] = 1;
		values[0][0] = 1;
		
		values[0] = new int[4];
		values[0][0] = 1;
		values[0][0] = 1;
		values[0][0] = 1;
		
		for (int [] arr : values) {
			for(int v:arr) {
				System.out.print(v+"\t"););
			}
			System.out.println();
		}

	}

}
