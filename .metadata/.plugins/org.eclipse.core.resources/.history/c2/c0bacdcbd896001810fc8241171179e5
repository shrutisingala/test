
public class Demo1D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] numbers;
		numbers = new int[5];
		numbers[0] = 100;
		numbers[1] = 200;
		numbers[2] = 300;
		numbers[3] = 400;
		numbers[4] = 500;
		
		int sum;
		sum = numbers[0] + numbers[1] + numbers[2] + numbers[3] + numbers[4];
		
		System.out.println("Sum of the array: " + sum);

	}

}
