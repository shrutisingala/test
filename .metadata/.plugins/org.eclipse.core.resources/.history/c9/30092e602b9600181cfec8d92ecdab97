
public class Person {
	
	private int age;
	private String name;
	
	//access_specifier return_type func_name()
	public void display() {
		System.out.println("Name:" + name + "\nAge:" + age);
	}
	
	public int Person() {
		age=21;
		name="Shruti";
	}
	
	public Person(int a, String n) {
		age=a;
		name=n;
	}
}
